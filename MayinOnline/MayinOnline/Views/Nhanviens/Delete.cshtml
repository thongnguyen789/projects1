@model MayinOnline.Models.Nhanvien

@{
    ViewData["Title"] = "Delete";
}

<h1 class="publid">Delete</h1>

<h3>Bạn chắc chắn muốn xóa nhân viên này?</h3>
<div>
    <h4>Nhân viên</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Tên Nhân Viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Ten)
        </dd>
        <dt class="col-sm-2">
            Điện Thoại
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DienThoai)
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Mật Khẩu
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MatKhau)
        </dd>
        <dt class="col-sm-2">
            Mã Chức Vụ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MaCvNavigation.Ten)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="MaNv" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Quay lại danh sách</a>
    </form>
</div>
