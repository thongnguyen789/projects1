@model MayinOnline.Models.Nhanvien

@{
    ViewData["Title"] = "Details";
}

<h1 class="publid">Details</h1>

<div>
    <h4>Nhân viên</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Tên Nhân Viên
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Ten)
        </dd>
        <dt class="col-sm-2">
            Điện Thoại
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DienThoai)
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Mật Khẩu
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MatKhau)
        </dd>
        <dt class="col-sm-2">
            Mã Chức Vụ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MaCvNavigation.Ten)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.MaNv">Sửa</a>
    <a asp-action="Index">Quay lại danh sách</a>
</div>
